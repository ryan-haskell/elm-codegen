{
  "name": "codegen",
  "version": "1.0.0",
  "description": "how to generate Elm with Elm (and NodeJS)",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "npm install && npm run build && npm run generate",
    "build": "elm make src/Main.elm --output=dist/elm.js --optimize",
    "generate": "node index.js",
    "test": "elm-test",
    "test:watch": "elm-test --watch"
  },
  "keywords": [],
  "author": "Ryan Haskell-Glatz",
  "license": "ISC",
  "devDependencies": {
    "elm": "0.19.1-3",
    "elm-test": "0.19.1-revision2"
  }
}
